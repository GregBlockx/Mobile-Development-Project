<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape
            android:padding="50dp"
            android:shape="rectangle">
            <corners android:radius="10.5dp" />
            <solid android:color="@color/colorLightBrown" />
        </shape>
    </item>
    <item
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp">
        <bitmap
            android:src="@drawable/hunger"
            android:tint="@color/colorDarkBrown" />
    </item>
</layer-list>