<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.blockx.greg.becomerich">

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <supports-screens
            android:anyDensity="true"
            android:compatibleWidthLimitDp="4"
            android:largeScreens="true"
            android:largestWidthLimitDp="20"
            android:normalScreens="true"
            android:requiresSmallestWidthDp="4"
            android:resizeable="true"
            android:smallScreens="true"
            android:xlargeScreens="true" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WorkActivity" />
        <activity android:name=".MarketActivity" />
        <activity android:name=".EducationActivity" />
        <activity android:name=".DoWorkActivity" />
        <activity android:name=".HungerActivity"></activity>
        <activity android:name=".DoCriminalJobActivity" />
        <activity android:name=".ChooseResidencyActivity" />
        <activity android:name=".ChooseTransportActivity" />
        <activity android:name=".ChooseEducationActivity" />
        <activity android:name=".ChooseCriminalSkillsActivity"></activity>
    </application>

</manifest>